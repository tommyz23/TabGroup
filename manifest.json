{
    "name": "Group your tabs",
    "description": "auto-group your tabs as configed",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "./popup/popup.html"
    },
    "background": {
        "service_worker": "./background/background.js",
        "type": "module"
    },
    "permissions": ["tabs", "storage"]
}
